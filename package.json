{
  "name": "pos-web-app",
  "version": "1.0.1",
  "description": "A simple POS web application with JSON database",
  "main": "server.js",
  "bin": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:win": "pkg . --targets node18-win-x64 --out-path dist",
    "build:win:run": "npm run build:win && dist\\pos-web-app.exe",
    "build:win:run-dev": "npm run build:win && set POS_TRIAL_ENABLED=false&& dist\\pos-web-app.exe"
  },
  "keywords": [
    "pos",
    "point-of-sale",
    "express",
    "json-database"
  ],
  "author": "Your Name",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "pkg": "^5.8.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "pkg": {
    "assets": [
      "public/**/*",
      "data/**/*"
    ],
    "scripts": [
      "server.js"
    ],
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "dist",
    "compression": "GZip"
  }
}